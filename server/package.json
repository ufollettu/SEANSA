{
  "name": "server",
  "version": "1.0.0",
  "scripts": {
    "start": "node ./bin/www",
    "testserver": "mocha --timeout 20000",
    "debug": "node --nolazy --inspect-brk=9229 ./bin/www",
    "dbinit": "node_modules/.bin/sequelize db:migrate && sequelize db:seed:all",
    "dbreset": "node_modules/.bin/sequelize db:migrate:undo:all && sequelize db:migrate && sequelize db:seed:all",
    "dbreload": "node_modules/.bin/sequelize db:seed:undo:all && sequelize db:seed:all",
    "migrations": "node_modules/.bin/sequelize db:migrate",
    "undomigrate": "node_modules/.bin/sequelize db:migrate:undo:all",
    "seeders": "node_modules/.bin/sequelize db:seed:all",
    "undoseed": "node_modules/.bin/sequelize db:seed:undo:all"
  },
  "private": true,
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "debug": "~2.6.9",
    "dotenv": "^6.0.0",
    "express": "~4.16.0",
    "handlebars": "^4.0.11",
    "hbs": "^4.0.1",
    "http-errors": "~1.6.2",
    "jsonwebtoken": "^8.3.0",
    "jwt-decode": "^2.2.0",
    "locutus": "^2.0.9",
    "moment": "^2.22.2",
    "morgan": "~1.9.0",
    "multer": "^1.3.1",
    "mysql2": "^1.5.3",
    "nodemailer": "^4.6.8",
    "parse-error": "^0.2.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "request-ip": "^2.1.1",
    "sequelize": "^4.37.10",
    "sequelize-cli": "^4.0.0",
    "serve-favicon": "^2.5.0",
    "stream": "0.0.2",
    "utf8-bytes": "0.0.1",
    "validator": "^10.3.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "codelyzer": "~4.2.1",
    "mocha": "^5.2.0",
    "nodemon": "^1.17.5",
    "proxyquire": "^2.1.0",
    "sequelize-mock": "^0.10.2",
    "sequelize-test-helpers": "^1.0.4",
    "sinon": "^6.1.5",
    "supertest": "^3.1.0"
  }
